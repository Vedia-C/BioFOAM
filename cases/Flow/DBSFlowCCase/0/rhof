FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rhof;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{

   

    left
    {
         type            zeroGradient;
    }

    right
    {
         type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    top
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

