FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    

    left
    {
     //type  zeroGradient;
         type            fixedValue;
     value	       uniform (0.000000001 0 0);
    }
    
    right
    {
       type  zeroGradient;
//type            fixedValue;
  //     value	       uniform (0 0 0);
    }

   
 bottom
    {
       type            cyclic;
    }
    top
    {
        type            cyclic;
    }
   
    frontAndBack
    {
        type            empty;
    }
}


