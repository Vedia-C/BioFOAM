    }*/
CbyC0
  {
        solver          GAMG;
        tolerance       1e-06;
        relTol          0.1;
        smoother        GaussSeidel;
    }

MbbyMmax
  {
        solver          GAMG;
        tolerance       1e-06;
        relTol          0.1;
        smoother        GaussSeidel;
    }

    pFinal
    {
        $p;
        tolerance       1e-06;
        relTol          0;
    }

CbyC0Final
{
        $CbyC0;
        tolerance       1e-06;
        relTol          0;
    }
MbbyMmaxFinal
{
        $MbbyMmaxFinal;
        tolerance       1e-06;
        relTol          0;
        solver          GAMG;
        smoother        GaussSeidel;
    }


    "(U|k|epsilon|omega|R|nuTilda)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-05;
        relTol          0;
    }
}

PISO
{
    momentumPredictor   yes;
    nCorrectors     2;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
}


